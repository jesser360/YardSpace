
<div class="main">
  <ul class="nav navbar-nav navbar-right">
  <% if logged_in? %>
  <li><%= link_to 'Log out', logout_path, method: :delete %></li>
  </li>
  <% else %>
  <li><%= link_to 'Log in', login_path %></li>
  <li><%= link_to 'Sign up', signup_path %></li>
  <% end %>
  </ul>

  <div class="container">
    <% if @user %>
      <p>Hello, <%=@user.email%></p>
      <%end%>

    <p>Welcome to YardSpace</p>

  </div>



</div>
